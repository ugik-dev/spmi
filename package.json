        "sass-loader": "^11.0.1"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.0.0",
        "bootstrap": "^4.6.0",
        "datatables.net": "^1.11.5",
        "datatables.net-bs4": "^1.13.8",
        "datatables.net-buttons-bs4": "^2.4.2",
        "datatables.net-searchpanes-bs4": "^2.2.0",
